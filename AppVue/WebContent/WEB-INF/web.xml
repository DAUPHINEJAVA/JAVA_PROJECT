<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>AppVue</display-name>
  <welcome-file-list>
    <welcome-file>log.html</welcome-file>
    <welcome-file>log.htm</welcome-file>
    <welcome-file>log.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PublicationServlet</servlet-name>
    <servlet-class>app.vue.servlets.PublicationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PublicationServlet</servlet-name>
    <url-pattern>/PublicationServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>OuvertureServlet</servlet-name>
    <servlet-class>app.vue.servlets.OuvertureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OuvertureServlet</servlet-name>
    <url-pattern>/OuvertureServlet</url-pattern>
  </servlet-mapping>
</web-app>